{
  "name": "remix-payloadcms",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:remix": "remix vite:build",
    "build:payload": "payload build",
    "build:server": "esbuild --bundle server.ts --platform=node --packages=external --format=esm --outfile=build/server.js",
    "build:payload-config": "esbuild  --bundle payload.config.ts --platform=node --packages=external --outfile=build/payload.config.cjs",
    "dev": "tsx server.ts",
    "check": "run-s typecheck lint",
    "format": "prettier --write .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production PAYLOAD_CONFIG_PATH=build/payload.config.cjs node ./build/server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@iconify/react": "^4.1.1",
    "@paralect/node-mongo": "^3.2.0",
    "@payloadcms/bundler-vite": "^0.1.7",
    "@payloadcms/db-mongodb": "^1.7.2",
    "@payloadcms/richtext-lexical": "^0.11.3",
    "@remix-run/express": "^2.11.2",
    "@remix-run/node": "^2.11.2",
    "@remix-run/react": "^2.11.2",
    "@remix-run/serve": "^2.11.2",
    "@remix-validated-form/with-zod": "^2.0.7",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "isbot": "^4.1.0",
    "lucide-react": "^0.379.0",
    "morgan": "^1.10.0",
    "payload": "^2.26.0",
    "qs": "^6.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-validated-form": "^5.1.5",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.11.2",
    "@remix-run/eslint-config": "^2.9.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "esbuild": "0.23.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
